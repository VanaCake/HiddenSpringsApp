<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hiddenspringsapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HiddenSpringsApp">
        <activity android:name=".ServerRatio"
            android:parentActivityName=".MainActivity"
            android:label="Server Ratio"></activity>
        <activity android:name=".Counters"
            android:parentActivityName=".MainActivity"
            android:label="Counters"></activity>
        <activity android:name=".FriyayGames"
            android:parentActivityName=".MainActivity"
            android:label="FriYAY Games"></activity>
        <activity android:name=".FriyayParticipants"
            android:parentActivityName=".MainActivity"
            android:label="FriYAY Participants"></activity>
        <activity android:name=".EventsList"
            android:parentActivityName=".MainActivity"
            android:label="Events List"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>